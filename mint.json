{
    "$schema": "https://mintlify.com/schema.json",
    "name": "LogX Network",
    "logo": {
        "dark": "/logo/dark.svg",
        "light": "/logo/light.svg"
    },
    "favicon": "/favicon.svg",
    "colors": {
        "primary": "#07D6B0",
        "light": "#07D6B0",
        "dark": "#0D9373",
        "anchors": {
            "from": "#0D9373",
            "to": "#07D6B0"
        }
    },
    "tabs": [
        {
            "name": "API Reference",
            "url": "api-reference"
        }
    ],
    "navigation": [
        {
            "group": "Get Started",
            "pages": ["introduction", "quickstart", "development"]
        },
        {
            "group": "Essentials",
            "pages": [
                "essentials/markdown",
                "essentials/code",
                "essentials/images",
                "essentials/settings",
                "essentials/navigation",
                "essentials/reusable-snippets"
            ]
        },
        {
            "group": "API Documentation",
            "pages": [
                "api-reference/introduction",
                "api-reference/authentication",
                {
                    "group": "Registration",
                    "pages": ["api-reference/endpoint/register-account"]
                },
                {
                    "group": "Markets",
                    "pages": [
                        "api-reference/endpoint/fetch-supported-markets",
                        "api-reference/endpoint/fetch-funding-rates",
                        "api-reference/endpoint/fetch-quotes"
                    ]
                },
                {
                    "group": "Order Management",
                    "pages": [
                        "api-reference/endpoint/create-order",
                        "api-reference/endpoint/delete-order",
                        "api-reference/endpoint/fetch-orders"
                    ]
                },
                {
                    "group": "Positions",
                    "pages": ["api-reference/endpoint/fetch-positions"]
                },
                {
                    "group": "Withdraw",
                    "pages": [
                        "api-reference/endpoint/withdrawable-balance",
                        "api-reference/endpoint/withdraw-token-balance"
                    ]
                },
                {
                    "group": "Nonce",
                    "pages": ["api-reference/endpoint/fetches-nonce"]
                },
                {
                    "group": "User data",
                    "pages": ["api-reference/endpoint/user-data"]
                }
            ]
        }
    ],
    "footerSocials": {
        "x": "https://x.com/mintlify",
        "github": "https://github.com/mintlify",
        "linkedin": "https://www.linkedin.com/company/mintsearch"
    }
}
